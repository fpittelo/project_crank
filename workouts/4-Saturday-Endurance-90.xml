<workout_file>
    <author>COACH</author>
    <name>4-Saturday-Endurance-90</name>
    <description>Total duration: 90 minutes. A longer session to improve fatigue resistance and aerobic base. Based on FTP of 228W.</description>
    <sportType>bike</sportType>
    <tags>
        <tag name="ENDURANCE"/>
    </tags>
    <workout>
        <Warmup Duration="420" PowerLow="0.37" PowerHigh="0.70">
            <textevent timeoffset="0" message="Let's begin the 7-minute warm-up ramp."/>
            <textevent timeoffset="410" message="Get ready for the activators."/>
        </Warmup>
        <IntervalsT Repeat="3" OnDuration="30" OnPower="0.55" OffDuration="30" OffPower="1.01">
            <textevent timeoffset="0" message="Activators: 30 seconds easy, 30 seconds at FTP."/>
            <textevent timeoffset="170" message="Activators complete. Main set is next."/>
        </IntervalsT>
        <SteadyState Duration="4500" Power="0.70">
            <textevent timeoffset="0" message="Main set: 75 minutes of steady endurance at 160W."/>
            <textevent timeoffset="10" message="Focus on smooth pedal stroke and keeping in Zone 2."/>
            <textevent timeoffset="600" message="Vary your cadence every 10 mins (e.g. 1 min @ 100rpm)."/>
            <textevent timeoffset="1200" message="20 minutes down used."/>
            <textevent timeoffset="2250" message="Halfway through the main set."/>
            <textevent timeoffset="4490" message="Almost done with the long ride."/>
        </SteadyState>
        <Cooldown Duration="300" PowerLow="0.55" PowerHigh="0.37">
            <textevent timeoffset="0" message="Extended cool-down."/>
            <textevent timeoffset="290" message="Workout complete. Great foundation work."/>
        </Cooldown>
    </workout>
</workout_file>
