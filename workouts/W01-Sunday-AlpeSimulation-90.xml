<workout_file>
    <author>COACH</author>
    <name>W01-Sunday-AlpeSimulation-90</name>
    <description>Total duration: 90 minutes. Simulateur Alpe du Zwift. On va chercher le D+ Fred ! Basé sur un FTP de 228W.</description>
    <sportType>bike</sportType>
    <tags>
        <tag name="CLIMBING"/>
        <tag name="TEMPO"/>
    </tags>
    <workout>
        <Warmup Duration="420" PowerLow="0.37" PowerHigh="0.70">
            <textevent timeoffset="0" message="Salut grimpeur ! Prêt pour l'ascension ?"/>
            <textevent timeoffset="120" message="Aujourd'hui on simule l'Alpe. Mental d'acier requis !"/>
            <textevent timeoffset="300" message="Prépare les jambes, ça va monter."/>
            <textevent timeoffset="410" message="Petits activateurs avant d'attaquer la pente."/>
        </Warmup>
        <IntervalsT Repeat="3" OnDuration="30" OnPower="0.55" OffDuration="30" OffPower="1.01">
            <textevent timeoffset="0" message="30s cool, 30s FTP. On y va !"/>
            <textevent timeoffset="30" message="Allez Fred !"/>
            <textevent timeoffset="90" message="En danseuse si tu veux !"/>
            <textevent timeoffset="150" message="Dernier coup de cul !"/>
            <textevent timeoffset="170" message="Ok, au pied du mur."/>
        </IntervalsT>
        <SteadyState Duration="600" Power="0.75">
            <textevent timeoffset="0" message="On assure l'approche. 10 minutes Tempo à 170W."/>
            <textevent timeoffset="300" message="Trouve ton rythme de croisière."/>
            <textevent timeoffset="590" message="Attention Fred, première rampe dans 10 secondes !"/>
        </SteadyState>
        <SteadyState Duration="900" Power="0.92">
            <textevent timeoffset="0" message="Montée 1/3. 15 minutes à 210W. C'est parti !"/>
            <textevent timeoffset="180" message="Imagine les premiers virages de l'Alpe. 21, 20..."/>
            <textevent timeoffset="450" message="Tu es solide Fred. Le souffle est maîtrisé."/>
            <textevent timeoffset="720" message="Allez, plus que 3 minutes pour ce tronçon."/>
            <textevent timeoffset="890" message="Sommet intermédiaire !"/>
        </SteadyState>
        <SteadyState Duration="300" Power="0.55">
            <textevent timeoffset="0" message="Descente / Récup. 5 minutes à 125W."/>
            <textevent timeoffset="150" message="Profite, bois, secoue les jambes."/>
            <textevent timeoffset="290" message="Attention, la pente se redresse !"/>
        </SteadyState>
        <SteadyState Duration="900" Power="0.92">
            <textevent timeoffset="0" message="Montée 2/3. 15 minutes. On retourne au charbon !"/>
            <textevent timeoffset="300" message="Allez Fred, grimpe ! Grimpe !"/>
            <textevent timeoffset="600" message="C'est dur ? C'est normal ! C'est là que tu progresses."/>
            <textevent timeoffset="750" message="Imagine la foule qui t'encourage ! Allez Fred !"/>
            <textevent timeoffset="890" message="Ouf ! Bien joué."/>
        </SteadyState>
        <SteadyState Duration="300" Power="0.55">
            <textevent timeoffset="0" message="Récupération. 5 minutes."/>
            <textevent timeoffset="240" message="Prépare-toi pour l'assaut final Fred."/>
            <textevent timeoffset="290" message="Dernier gros effort de la semaine !"/>
        </SteadyState>
        <SteadyState Duration="900" Power="0.92">
            <textevent timeoffset="0" message="Montée 3/3. 15 minutes jusqu'au sommet !"/>
            <textevent timeoffset="180" message="Donne tout ce qu'il te reste !"/>
            <textevent timeoffset="450" message="La ligne d'arrivée n'est plus très loin dans ta tête !"/>
            <textevent timeoffset="720" message="Les derniers lacets ! Allez Fred, relance !"/>
            <textevent timeoffset="840" message="Dernière minute ! A bloc !"/>
            <textevent timeoffset="890" message="SOMMET ! TU ES LE ROI !"/>
        </SteadyState>
        <Cooldown Duration="900" PowerLow="0.55" PowerHigh="0.37">
            <textevent timeoffset="0" message="Long retour au calme mérité. 15 minutes."/>
            <textevent timeoffset="300" message="Tu as vaincu la montagne aujourd'hui."/>
            <textevent timeoffset="600" message="Laisse redescendre tranquillement."/>
            <textevent timeoffset="890" message="Bravo Fred. Séance épique terminée."/>
        </Cooldown>
    </workout>
</workout_file>
